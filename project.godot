[application]

config/name="eBrew POC"
config/description="This is a proof of concept implimentation of the eBrew Store mockups from 2021."
run/main_scene="res://Scenes/ui.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")
boot_splash/bg_color=Color(0.137255, 0.478431, 0.780392, 1)
